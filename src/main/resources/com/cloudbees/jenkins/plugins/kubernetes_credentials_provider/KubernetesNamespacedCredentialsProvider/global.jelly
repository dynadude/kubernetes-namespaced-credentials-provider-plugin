<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:section title="Kubernetes Credentials Additional Namespaces">
        <f:repeatable field="additionalNamespaces"
                        add="${%Add Namespace}">
            <br/>
            <f:entry field="name">
                <f:textbox default="" />
            </f:entry>
            <f:repeatableDeleteButton value="${%Delete Namespace}" />
        </f:repeatable>
    </f:section>
</j:jelly>